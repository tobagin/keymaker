<?xml version="1.0" encoding="UTF-8"?>
<!-- Gettext domain verified 2025-10-12 Phase 7: Must match meson.project_name() in meson.build -->
<schemalist gettext-domain="keysmith">
  <schema id="@APP_ID@" path="/@APP_PATH@/">
    <key name="window-width" type="i">
      <default>800</default>
      <summary>Default window width</summary>
      <description>The default width of the main window</description>
    </key>
    
    <key name="window-height" type="i">
      <default>600</default>
      <summary>Default window height</summary>
      <description>The default height of the main window</description>
    </key>
    
    <key name="window-maximized" type="b">
      <default>false</default>
      <summary>Window maximized state</summary>
      <description>Whether the window is maximized</description>
    </key>
    
    <key name="default-key-type" type="s">
      <default>'ed25519'</default>
      <summary>Default SSH key type</summary>
      <description>The default SSH key type to use when generating new keys</description>
    </key>
    
    <key name="default-rsa-bits" type="i">
      <default>4096</default>
      <summary>Default RSA key size</summary>
      <description>The default bit size for RSA keys</description>
    </key>
    
    <key name="default-ecdsa-curve" type="i">
      <default>256</default>
      <summary>Default ECDSA curve</summary>
      <description>The default curve size for ECDSA keys (256, 384, or 521)</description>
    </key>
    
    <key name="default-comment" type="s">
      <default>''</default>
      <summary>Default key comment</summary>
      <description>The default comment to use for new SSH keys</description>
    </key>
    
    <key name="use-passphrase-by-default" type="b">
      <default>true</default>
      <summary>Use passphrase by default</summary>
      <description>Whether to enable passphrase protection by default for new keys</description>
    </key>
    
    <key name="auto-refresh-interval" type="i">
      <default>30</default>
      <summary>Auto refresh interval</summary>
      <description>Interval in seconds for automatically refreshing the key list (0 to disable)</description>
    </key>
    
    <key name="show-fingerprints" type="b">
      <default>true</default>
      <summary>Show fingerprints</summary>
      <description>Whether to show key fingerprints in the key list</description>
    </key>
    
    <key name="confirm-deletions" type="b">
      <default>true</default>
      <summary>Confirm key deletions</summary>
      <description>Whether to show confirmation dialog before deleting keys</description>
    </key>
    
    <key name="theme" type="s">
      <default>'auto'</default>
      <summary>Theme preference</summary>
      <description>The theme to use: 'auto' (system), 'light', or 'dark'</description>
    </key>
    
    <key name="last-version-shown" type="s">
      <default>""</default>
      <summary>Last version for which release notes were shown</summary>
      <description>Tracks the last application version for which release notes were displayed to avoid showing them repeatedly</description>
    </key>
    
    <key name="key-service-mappings" type="aa{sv}">
      <default>[]</default>
      <summary>SSH key service mappings</summary>
      <description>Stores associations between SSH keys and services they are used for</description>
    </key>
    
    <key name="rotation-plans" type="aa{sv}">
      <default>[]</default>
      <summary>SSH key rotation plans</summary>
      <description>Stores rotation plan configurations for SSH keys including targets and settings</description>
    </key>
    
    <key name="tunnel-configurations" type="aa{sv}">
      <default>[]</default>
      <summary>SSH tunnel configurations</summary>
      <description>Stores saved SSH tunnel configurations</description>
    </key>
    
    <key name="preferred-terminal" type="s">
      <default>'auto'</default>
      <summary>Preferred terminal emulator</summary>
      <description>The preferred terminal emulator to use for SSH connections: 'auto' (detect automatically) or specific terminal name</description>
    </key>
    
    <key name="auto-run-diagnostics" type="b">
      <default>false</default>
      <summary>Auto-run diagnostics</summary>
      <description>Whether to automatically run diagnostic tests when they are added</description>
    </key>
    
    <key name="diagnostics-retention-days" type="i">
      <default>30</default>
      <summary>Diagnostics retention period</summary>
      <description>Number of days to keep diagnostic history entries before automatic cleanup</description>
    </key>

    <key name="cloud-provider-github-connected" type="b">
      <default>false</default>
      <summary>GitHub connection status</summary>
      <description>Whether a GitHub account is currently connected</description>
    </key>

    <key name="cloud-provider-github-username" type="s">
      <default>''</default>
      <summary>GitHub username</summary>
      <description>The connected GitHub username</description>
    </key>

    <key name="cloud-provider-gitlab-connected" type="b">
      <default>false</default>
      <summary>GitLab connection status</summary>
      <description>Whether a GitLab account is currently connected</description>
    </key>

    <key name="cloud-provider-gitlab-username" type="s">
      <default>''</default>
      <summary>GitLab username</summary>
      <description>The connected GitLab username</description>
    </key>

    <key name="cloud-provider-gitlab-instance-url" type="s">
      <default>'https://gitlab.com'</default>
      <summary>GitLab instance URL</summary>
      <description>The URL of the GitLab instance (GitLab.com or self-hosted)</description>
    </key>

    <key name="cloud-provider-gitlab-allow-self-signed" type="b">
      <default>false</default>
      <summary>Allow self-signed certificates</summary>
      <description>Whether to allow self-signed certificates for self-hosted GitLab instances (insecure)</description>
    </key>

    <key name="cloud-provider-gitlab-client-id" type="s">
      <default>''</default>
      <summary>GitLab OAuth Client ID</summary>
      <description>The OAuth application client ID for GitLab</description>
    </key>

    <key name="cloud-provider-gitlab-client-secret" type="s">
      <default>''</default>
      <summary>GitLab OAuth Client Secret</summary>
      <description>The OAuth application client secret for GitLab</description>
    </key>

    <key name="cloud-provider-cache" type="s">
      <default>'{}'</default>
      <summary>Cloud provider key cache</summary>
      <description>JSON cache of cloud provider SSH keys for offline viewing</description>
    </key>

    <key name="cloud-provider-show-deploy-warning" type="b">
      <default>true</default>
      <summary>Show deployment warning</summary>
      <description>Whether to show security warning before deploying keys to cloud</description>
    </key>
  </schema>

  <schema id="@APP_ID@.tunneling" path="/@APP_PATH@/tunneling/">
    <key name="configurations" type="aa{sv}">
      <default>[]</default>
      <summary>SSH tunnel configurations</summary>
      <description>Stores saved SSH tunnel configurations</description>
    </key>
  </schema>
</schemalist>